doctype html
html
   head
      title all information
   body
      p(style="color:blue") student details
      h1(style="color:red") registration
      table(border="2" cellpadding="10")
        thead 
            th your name
                button sort
            th email 
                button sort
            th phone number
                button sort
            th city
                button sort
            th technology 
                button sort
            th company
                button sort 
        each val in allstudents 
            tr 
                td #{val.yourname}
                td #{val.email}
                td #{val.phonenumber}
                td #{val.city}
                td #{val.technology}
                td #{val.company}
